{
  "env": {
    "browser": true,
    "es2022": true,
    "cypress/globals": true
  },
  "extends": [
    "eslint:recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 2022,
    "sourceType": "module"
  },
  "plugins": ["cypress"],
  "ignorePatterns": [
    "node_modules/",
    "tests/performance/k6/dist/"
  ],
  "rules": {
    "no-unused-vars": ["warn", { "argsIgnorePattern": "^_" }],
    "no-undef": "warn"
  }
}



